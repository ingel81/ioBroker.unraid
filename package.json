{
  "name": "iobroker.unraid",
  "version": "0.7.1",
  "description": "Monitor and control Unraid servers via GraphQL API",
  "author": {
    "name": "ingel81",
    "email": "ingel81@sgeht.net"
  },
  "homepage": "https://github.com/ingel81/ioBroker.unraid",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "unraid",
    "monitoring",
    "unraid-api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ingel81/ioBroker.unraid.git"
  },
  "engines": {
    "node": ">= 20"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@iobroker/adapter-core": "^3.3.2",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.6",
    "undici": "^7.16.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^5.0.0",
    "@alcalzone/release-script-plugin-iobroker": "^4.0.0",
    "@alcalzone/release-script-plugin-license": "^4.0.0",
    "@alcalzone/release-script-plugin-manual-review": "^4.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@iobroker/adapter-dev": "^1.5.0",
    "@iobroker/adapter-react-v5": "^8.0.16",
    "@iobroker/eslint-config": "^2.2.0",
    "@iobroker/testing": "^5.2.2",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^25.0.3",
    "@types/proxyquire": "^1.3.31",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "proxyquire": "^2.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).{json,json5}",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "build/"
  ],
  "scripts": {
    "prebuild": "rimraf admin/build",
    "build": "npm run build:ts && build-adapter react",
    "build:ts": "tsc -p tsconfig.build.json",
    "watch": "build-adapter react --watch",
    "watch:ts": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.{js,ts},*.test.{js,ts},test/**/test!(PackageFiles|Startup).{js,ts}}\"",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint -c eslint.config.mjs src admin/src",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/ingel81/ioBroker.unraid/issues"
  },
  "readmeFilename": "README.md"
}
